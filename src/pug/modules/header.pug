- var navLinks = [{ link: "#", name: "1"},{ link: "#", name: "2"}]

mixin navLink(link,name,last)
    li
        if(last)
            a(href=link).nav-link.last-link=name
        else
            a(href=link).nav-link=name


header
   .header
        ul.nav
           +navLink('#','1')
           +navLink('#','2')
           +navLink('#','3')
           +navLink('#','4',true)
div
    .test_cycle
        ul.nav
            for i in [1,2,3]
                li
                    a(href="").nav-link=i
div
    .test_cycle_2
        ul.nav
            for i in navLinks
                li
                    a(href=i.link).nav-link=i.name